{
    "opts": {
        "destination": "./docs/",
        "readme": "docs/intro.md",
        "template": "node_modules/docdash"
    },
    "source": {
        "include": [
            "index.js",
            "src/relational.js",
            "src/query.js"
        ]
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "referenceTitle": "jsldb",
        "collapse": true,
        "resources": {
            "Queries": "tutorial-queries.html",
            "Schemas": "tutorial-schemas.html",
            "Glossary": "tutorial-glossary.html"
        }
    },
    "docdash": {
        "sort": false,
        "sectionOrder": [
            "Tutorials", "Modules", "Classes"
        ]
    }
}
