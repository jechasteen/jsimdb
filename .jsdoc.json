{
    "opts": {
        "destination": "./docs/",
        "readme": "README.md",
        "template": "node_modules/jsdoc-template"
    },
    "templates": {
        "referenceTitle": "jsldb",
        "disableSort": false,
        "collapse": true,
        "resources": {
            "Wikipedia": "https://en.wikipedia.org/wiki/Relational_database"
        }
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "sourceType": "module",
    "source": {
        "include": [
            "index.js",
            "src/relational.js"
        ]
    }
}
